<h1>Wikinimous</h1>
<h3><%= link_to "New Article", new_article_path %></h3>
<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <% @articles.each do |article| %>
      <article>
        <h2>
          <%= link_to article.title, article_path(article.id)%>
        </h2>
        <%= article.content %>
      </article>
      <% end %>
    </div>
  </div>
</div>
